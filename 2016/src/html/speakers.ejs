  speakers: [
    'douglascrockford',
    'jasnell',
    'bmeck',
    'zcbenz',
    'yoshuawuyts',
    'mafintosh',
    'a0viedo',
    'watson',
    'teppeis',
    'bahmutov',
    'fritzvd',
    'BarakChamo',
    'RichardLitt',
    'kazupon',
    'yoshidan',
    'nkzawa',
