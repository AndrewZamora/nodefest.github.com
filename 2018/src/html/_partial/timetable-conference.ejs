<%
var schedule = sessions.day1;
%>
<div class="timetable -conference">

  <div class="timetable-caption">
    <h3 class="timetable-title">
      Day 1 <span class="timetable-venue">at Nov 23th</span>
    </h3>
  </div>

  <div class="timetable-notice">
    <p>
      TODO: Room CおよびRoom Dでも、...
    </p>
  </div>
  <div class="timetable-header">
    <div class="timetable-time"></div>
    <div class="timetable-section">
      <% schedule.head.forEach(h => { %>
      <p><%- h %></p>
      <% }); %>
    </div>
  </div>

  <% schedule.body.forEach(row => { %>
  <div class="timetable-row">
    <div class="timetable-time">
      <p><%- row.time %></p>
    </div>
    <div class="timetable-content">
      <% row.sessions.forEach(session => { %>
      <% if (session.isLT) { %>
      <div class="timetable-session">
        <p class="timetable-sessionTitle"><%- session.title %></p>
        <ul class="timetable-lts">
          <% session.talks.forEach(talk => { %>
          <li>
            <p class="timetable-ltsTitle"><%- talk.title %></p>
            <p class="timetable-ltsSpeaker"><%- talk.speaker %></p>
          </li>
          <% }); %>
        <ul>
      </div>
      <% } else { %>
      <div class="timetable-session">
        <p class="timetable-sessionTitle"><%- session.title %></p>
        <p class="timetable-sessionSpeaker"><%- session.speaker %></p>
      </div>
      <% } %>
      <% }); %>
    </div>
  </div>
  <% }); %>

</div>
